{
  "name": "cf-worker-ai",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "start": "wrangler dev --remote",
    "reset-db": "wrangler d1 execute database --command \"DELETE FROM documents\" && wrangler vectorize delete vector-index && wrangler vectorize create vector-index --metric=cosine --dimensions=768",
    "tail": "wrangler tail --format=pretty",
    "tunnel": "cloudflared tunnel --config .cloudflared/config.yml run ai-poc"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    "@cloudflare/ai": "^1.0.15",
    "cheerio": "^1.0.0-rc.12",
    "hono": "^3.7.4",
    "html-to-text": "^9.0.5",
    "langchain": "^0.0.165"
  }
}
